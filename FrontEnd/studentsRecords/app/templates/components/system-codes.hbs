<div class = "ui center aligned padded grid container">
  <div class = "ui basic segment">
    <div class = "ui huge header">
      System Codes
    </div>
  </div>
</div>

{{#if showDeleteGenderConfirmation}}
  {{delete-gender gender = currentGender showWindow = showDeleteGenderConfirmation}}
{{else if showDeleteResidencyConfirmation}}
  {{delete-residency residency = currentResidency showWindow = showDeleteResidencyConfirmation}}
{{else if showCourseCodeDeleteConfirmation}}
  {{delete-course-code courseCode = currentCourseCode showWindow = showCourseCodeDeleteConfirmation}}
{{else if showDeleteHighSchoolConfirmation}}
  {{delete-high-school highSchool = currentHighSchool showWindow = showDeleteHighSchoolConfirmation}}
{{else if showDeleteHighSchoolSubjectConfirmation}}
  {{delete-high-school-subject highSchoolSubject = currentHighSchoolSubject showWindow = showDeleteHighSchoolSubjectConfirmation}}
{{else if showDeleteTermCodeConfirmation}}
  {{delete-term-code termCode = currentTermCode showWindow = showDeleteTermCodeConfirmation}}
{{else if showDeleteCourseGroupingConfirmation}}
  {{delete-course-grouping courseGrouping = currentCourseGrouping showWindow = showDeleteCourseGroupingConfirmation}}
{{else if showAddCoursesModal}}
  {{add-new-course-grouping courseGroup=currentCourseGrouping showWindow = showAddCoursesModal}}
{{else if }}

{{/if}}

  <!-- Show data entry form -->
<div class = "ui grid" style = "margin:0">
    <div class = "sixteen wide column" style="padding-bottom:0">
        <div class = "ui secondary pointing fluid ten item menu">
            <div style="cursor: pointer;" class="active item" data-tab="Genders">Gender</div>
            <div style="cursor: pointer;" class="item" data-tab="Residencies">Residency</div>
            <div style="cursor: pointer;" class="item" data-tab="TermCodes">Term Codes</div>
            <div style="cursor: pointer;" class="item" data-tab="CourseCodes">Course Codes</div>
            <div style="cursor: pointer;" class="item" data-tab="CourseGroupings">Course Groupings</div>
            <div style="cursor: pointer;" class="item" data-tab="HighSchools">High Schools</div>
            <div style="cursor: pointer;" class="item" data-tab="HighSchoolSubjects">High School Subjects</div>
            <div style="cursor: pointer;" class="item" data-tab="Faculties">Faculties</div>
            <div style="cursor: pointer;" class="item" data-tab="Departments">Departments</div>
            <div style="cursor: pointer;" class="item" data-tab="ProgramAdmins">Faculty Administrators</div>
        </div>
    </div>
</div>

  <div class="ui tab segment active" data-tab="Genders">
    <table class="ui fixed table">
        <thead>
        <tr>
            <th class="eight wide">Gender Name</th>
            <th class="eight wide"><!--spacer--></th>
        </tr>
        </thead>
        {{#if (inline-compare genderModel.length '==' 0)}}
            <tr style = "height:100px">
            <td colspan = "2" class = "middle aligned">
            <div class = "dimmable">
              <div class = "ui active inverted dimmer">
                <div class = "ui text">No Genders Found</div>
              </div>
            </div>
            </td>
        </tr>
        {{else if (inline-compare genderModel.length '>' 0)}}
            {{#each genderModel as |gender|}}
            <tr>
                <td>
                    <div class="ui fluid input">
                        {{input type="text" value = gender.name}}
                    </div>
                </td>
                <td>
                    <div class="ui inverted buttons">
                        <button class="ui cstmLBlue inverted button" {{action "saveGender" gender}}>Save
                            &nbsp; <i class="save icon"></i>
                        </button>
                        <div class="divider"/>
                        <button class="ui red inverted button" {{action "deleteGender" gender}}>Delete
                            &nbsp; <i class="trash icon"></i>
                        </button>
                    </div>
                </td>
            </tr>
            {{/each}}
        {{else}}
            <tr style = "height:100px">
                <td colspan = "2" class = "middle aligned">
                <div class = "dimmable">
                  <div class = "ui active inverted dimmer">
                    <div class = "ui text loader">Loading Genders...</div>
                  </div>
                </div>
                </td>
            </tr>
        {{/if}}
        <tr>
            <td>
                <div class="ui fluid input">
                    {{input type="text" value = newGenderName placeholder="Gender Name"}}
                </div>
            </td>
            <td>
                <button class="ui inverted green button" {{action "addGender"}}> 
                Add &nbsp; <i class="add square icon"></i>
                </button>
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <div>
			        <label>{{{genderOutput}}}</label>
		        </div>
            </td>
        </tr>
    </table>
  </div>

  <div class="ui tab segment" data-tab="Residencies">
      <table class="ui fixed table">
        <thead>
            <tr>
            <th class="eight wide">Residency Name</th>
            <th class="eight wide"><!--spacer--></th>
