                <div class = "dimmable">
                  <div class = "ui active inverted dimmer">
                    <div class = "ui text loader">Loading Residencies...</div>
                  </div>
                </div>
                </td>
            </tr>
        {{/if}}
        <tr>
            <td>
                <div class="ui fluid input">
                    {{input type="text" value = newResidencyName placeholder = "Residency Name"}}
                </div>
            </td>
            <td>
                <button class="ui inverted green button" {{action "addResidency"}}> 
                Add &nbsp; <i class="add square icon"></i>
                </button>
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <div>
			        <label>{{{residencyOutput}}}</label>
		        </div>
            </td>
        </tr>
        </table>
  </div>

<div class="ui tab segment" data-tab="CourseCodes">
    <table class="ui table">
        <thead>
            <tr>
            <th class="two wide">Course Letter</th>
            <th class="three wide">Course Number</th>
            <th class="five wide">Course Name</th>
