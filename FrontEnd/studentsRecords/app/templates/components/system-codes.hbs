            </td>

            <td>
                <div class="ui fluid input">
                    {{input type="text" value = courseCode.courseNumber}}
                </div>
            </td>

            <td>
                <div class="ui fluid input">
                    {{input type="text" value = courseCode.name}}
                </div>
            </td>

            <td>
                <div class="ui fluid input">
                    {{input type="text" value = courseCode.unit}}
                </div>
            </td>

            <td>
                <div class="ui inverted buttons">
                    <button class="ui cstmLBlue inverted button" {{action "saveCourseCode" courseCode}}>Save
                        &nbsp; <i class="save icon"></i>
                    </button>
                    <div class="divider"/>
                    <button class="ui red inverted button" {{action "deleteCourseCode" courseCode}}>Delete
                        &nbsp; <i class="trash icon"></i>
                    </button>
                </div>
            </td>
        </tr>
        {{/each}}
        {{else}}
            <tr style = "height:100px">
                <td colspan = "5" class = "middle aligned">
                <div class = "dimmable">
                  <div class = "ui active inverted dimmer">
                    <div class = "ui text loader">Loading Course Codes...</div>
                  </div>
                </div>
                </td>
            </tr>
        {{/if}}
        <tr>
            <td>
                <div class="ui fluid input">
                    {{input type="text" value = newCourseCodeCourseLetter placeholder="Course Letter"}}
                </div>
            </td>
    
            <td>
                <div class="ui fluid input">
                    {{input type="text" value = newCourseCodeCourseNumber placeholder="Course Number"}}
                </div>
            </td>


            <td>
                <div class="ui fluid input">
                    {{input type="text" value = newCourseCodeName placeholder="Course Name"}}
                </div>
            </td>
                
            <td> 
                <div class="ui fluid input">
                    {{input type="text" value = newCourseCodeUnit placeholder="Course Unit"}}
                </div>
            </td>

            <td>
                <button class="ui inverted green button" {{action "addCourseCode"}}> 
                Add &nbsp; <i class="add square icon"></i>
                </button>
            </td>
        </tr>

        <tr>
            <td colspan="12">
                <div style="margin: auto; width: 50%;">
                  <div class="ui fluid pagination nine item menu">
                    {{#if (inline-compare courseCodePageNumber '<' 5)}}
                      <div class="disabled item"></div>
                    {{/if}}
                    {{#if (inline-compare courseCodePageNumber '>' 3)}}
                      <a class="item" {{action "switchCourseCodePage" 1}}>
                        1
                      </a> 
                    {{else}}
                      <div class="disabled item"></div>
                    {{/if}} 
                    {{#if (inline-compare courseCodePageNumber '>' 5)}}
                      <div class="disabled item">
                        ...
                      </div>
                    {{else}} 
                      {{#if (inline-compare courseCodePageNumber '===' 5)}}
                      <a class="item" {{action "switchCourseCodePage" 2}}>
                        2
                      </a> 
                      {{/if}} 
                    {{/if}}
                    {{#if (inline-compare courseCodePageNumber '>' 2)}}
                      <a class="item" {{action "switchCourseCodePage" (inline-math courseCodePageNumber '-' 2)}}>
                        {{inline-math courseCodePageNumber '-' 2}}
                      </a> 
                    {{else}}
                      <div class="disabled item"></div>
                    {{/if}}
                    {{#if (inline-compare courseCodePageNumber '>' 1)}}
                      <a class="item" {{action "switchCourseCodePage" (inline-math courseCodePageNumber '-' 1)}}>
                        {{inline-math courseCodePageNumber '-' 1}}
                      </a>
                    {{else}}
                      <div class="disabled item"></div>
                    {{/if}}

                    <a class="active item">
                      {{courseCodePageNumber}}
                    </a> 

                    {{#if (inline-compare courseCodePageNumber '<' totalCourseCodePages)}}
                      <a class="item" {{action "switchCourseCodePage" (inline-math courseCodePageNumber '+ ' 1)}}>
                        {{inline-math courseCodePageNumber '+ ' 1}}
                      </a>
                    {{else}}
                      <div class="disabled item"></div>
                    {{/if}}
                    {{#if (inline-compare courseCodePageNumber '<' (inline-math totalCourseCodePages '- ' 1))}}
                      <a class="item" {{action "switchCourseCodePage" (inline-math courseCodePageNumber '+ ' 2)}}>
                        {{inline-math courseCodePageNumber '+ ' 2}}
                      </a>
                    {{else}}
                      <div class="disabled item"></div>
                    {{/if}}
                    {{#if (inline-compare courseCodePageNumber '<' (inline-math totalCourseCodePages '- ' 4))}}
                      <div class="disabled item">
                        ...
                      </div>
                    {{else}}
                      {{#if (inline-compare courseCodePageNumber '===' (inline-math totalCourseCodePages ' - ' 4))}}
                        <a class="item" {{action "switchCourseCodePage" (inline-math totalCourseCodePages '- ' 1 )}}>
                          {{inline-math totalCourseCodePages '- ' 1 }}
                        </a>
                      {{/if}}
                    {{/if}}
                    {{#if (inline-compare courseCodePageNumber '<' (inline-math totalCourseCodePages '- ' 2))}}
                      <a class="item" {{action "switchCourseCodePage" totalCourseCodePages}}>
                        {{totalCourseCodePages}}
                      </a>
                    {{else}}
                      <div class="disabled item"></div>
                    {{/if}}
                    {{#if (inline-compare courseCodePageNumber '>' (inline-math totalCourseCodePages '-' 4))}}
                        <div class="disabled item"></div>
                    {{/if}}
                  </div>
                </div>
            </td>
        </tr>    
    </table>
 </div>

